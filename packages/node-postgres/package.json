{
  "name": "@aws/aurora-dsql-node-postgres-connector",
  "version": "0.1.4",
  "description": "An AWS Aurora DSQL connector with IAM authentication for node-postgres",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/awslabs/aurora-dsql-nodejs-connector"
  },
  "homepage": "https://github.com/awslabs/aurora-dsql-nodejs-connector",
  "keywords": ["aws", "aurora", "dsql", "postgres", "database", "iam", "authentication"],
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "scripts": {
    "clean": "rm -rf dist/ node_modules/",
    "build": "npm run build:dev",
    "build:dev": "npm run typecheck && tsdown \"src/**/*.ts\" --format cjs,esm --dts --sourcemap",
    "build:prod": "npm run typecheck && tsdown src/index.ts --format cjs,esm --dts",
    "typecheck": "tsc --noEmit",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "jest --testPathIgnorePatterns=integration",
    "test:integration": "NODE_OPTIONS='--experimental-vm-modules' jest --testPathPatterns=integration",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "lint": "eslint src/ test/",
    "lint:fix": "eslint src/ test/ --fix"
  },
  "files": [
    "dist/",
    "README.md"
  ],
  "dependencies": {
    "@aws-sdk/credential-providers": "^3.894.0",
    "@aws-sdk/dsql-signer": "^3.940.0"
  },
  "peerDependencies": {
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.0",
    "@types/pg": "^8.15.6",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "eslint": "^9.39.1",
    "eslint-plugin-header": "^3.1.1",
    "jest": "^30.2.0",
    "pg": "^8.16.3",
    "ts-jest": "^29.4.5",
    "tsdown": "^0.17.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
