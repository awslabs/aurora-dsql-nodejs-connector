{
  "name": "@aws/aurora-dsql-postgresjs-connector",
  "version": "0.1.2",
  "description": "An AWS Aurora DSQL connector with IAM authentication for Postgres.js",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/awslabs/aurora-dsql-nodejs-connector"
  },
  "homepage": "https://github.com/awslabs/aurora-dsql-nodejs-connector",
  "keywords": [
    "aws",
    "aurora",
    "dsql",
    "postgres",
    "database",
    "iam",
    "authentication"
  ],
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "scripts": {
    "clean": "rm -rf dist/ node_modules/",
    "build": "npm run build:dev",
    "build:dev": "npm run typecheck && tsdown \"src/**/*.ts\" --format cjs,esm --dts --sourcemap",
    "build:prod": "npm run typecheck && tsdown src/index.ts --format cjs,esm --dts",
    "typecheck": "tsc --noEmit",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "jest --testPathIgnorePatterns=integration",
    "test:integration": "NODE_OPTIONS='--experimental-vm-modules' jest --testPathPatterns=integration",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts test/**/*.ts",
    "lint:fix": "eslint src/**/*.ts test/**/*.ts --fix"
  },
  "files": [
    "dist/",
    "README.md"
  ],
  "dependencies": {
    "@aws-sdk/credential-providers": "^3.901.0",
    "@aws-sdk/dsql-signer": "^3.940.0",
    "async-mutex": "^0.5.0"
  },
  "peerDependencies": {
    "postgres": "^3.4.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^25.0.0",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "eslint": "^9.39.1",
    "eslint-plugin-header": "^3.1.1",
    "jest": "^30.2.0",
    "postgres": "^3.4.7",
    "ts-jest": "^29.4.5",
    "tsdown": "^0.17.2",
    "typescript": "^5.9.3"
  }
}
